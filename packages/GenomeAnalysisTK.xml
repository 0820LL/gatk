    <file>testdata/exampleBAM.bam</file>
    <file>testdata/exampleBAM.bam.bai</file>
    <file>testdata/exampleFASTA.fasta</file>
    <file>testdata/exampleFASTA.fasta.fai</file>
    <file>testdata/exampleFASTA.dict</file>
